{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
      "react/react-in-jsx-scope": "warn",
      "prettier/prettier": "off",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-debugger": "warn",
      "react/prop-types": "off",
      "react/no-unescaped-entities": "off",
      "no-empty-character-class": "warn",
      "no-empty": "warn",
      "no-undef": "warn"
  }
}